# API URL - Using /api will route through the Vite proxy
VITE_API_URL=/api
#VITE_API_URL=http://localhost:5000/api
VITE_NOTIFICATION_URL=ws://localhost:4000
VITE_API_VERSION_PREFIX=v1

# Feature flags
VITE_ENABLE_QUERY_FEATURE=true
VITE_ENABLE_TRENDS_ANALYSIS=true

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_REFRESH_INTERVAL=5
VITE_MAX_RECENT_QUERIES=10